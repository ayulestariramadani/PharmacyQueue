QMainWindow{
    background-color: #003468;
}
QLabel{
    font-family: 'Sagoe UI';
    color: white;
}
QLabel#title{
    font-family: 'Sagoe UI';
    font-weight: bold;
    font-size: 40px;
    color: #fff; 
    margin:0px;  
}
#date_txt{
    font-family: 'Sagoe UI';
    background-color: #001A33;
    padding: 20px;
    font-size: 17px;
}
QLabel#patient{
    font-family: 'Sagoe UI';
    font-size: 45px;
    color: black;
    padding: 5px;
    border: 1px solid #ccc;
    border-radius: 5px;
    background-color: #f9f9f9;
}
QListWidget{
    font-family: 'Sagoe UI';
    background-color: #003468;
    color: #fff;
    font-size: 40px;
}
#current_queue{
    background-color: #fFF;
}
#panggil_button{
    background-color: #0080FF;
    border-radius: 5px;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 15px;
    padding-right: 15px;

}
#patient_list_item {
    font-size: 20px;
    font-weight: -100;
}
QLabel#queue_title{
    font-weight: bold;
    font-size: 40px;
    color: #003468;   
}
#norm_label{
    font-family: 'Sagoe UI';
    font-size: 128px;
    font-weight: 700;
    color: black;
    margin:0px;
    padding:0px;
}
#name_label{
    font-family: 'Sagoe UI';
    font-size: 40px;
    font-weight: 700;
    color: #003468;
    margin:0px;
    padding:0px;
}
#search_widget{
    background-color: white;
    border-radius: 20px;
    border: 1px solid #919191;
}
#search_bar{
    border: none;
    background-color: white;
    color: black;
    padding: 5px; /* Padding for better look */
    outline: none; /* Remove the default focus frame */
}
#close_button{
    border: none;
    background: none;
    color: #919191;
    text-decoration: underline;
}
#close_button:hover {
    color: black;
}
#ext_txt{
    font-family: 'Sagoe UI';
}
QTableWidget {
    background-color: #003468;
    border: none;
    font-size: 14px;
}
QTableWidget::item {
    padding: 10px;
    border: none;
}
QHeaderView::section {
    background-color: #003468;
    padding: 5px;
    border: none;
    font-weight: bold;
    font-size: 14px
}